{
  "ts-node": {
    "transpileOnly": true,
    "transpiler": "ts-node/transpilers/swc-experimental"
  },
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "allowImportingTsExtensions": true,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "paths": {
      "~/*": ["./src/*"]
    }
  },
  "exclude": [],
